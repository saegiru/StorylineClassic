	<Button name="Storyline_ChoiceButton" virtual="true" hidden="false">
		<Size x="0" y="16"/>
		<Scripts>
			<OnEnter>
				Storyline_RefreshTooltipForFrame(self);
			</OnEnter>
			<OnLeave>
				Storyline_MainTooltip:Hide();
			</OnLeave>
			<PostClick>
				PlaySound("gsCharacterSelection","SFX");
			</PostClick>
		</Scripts>
		<HighlightTexture file="Interface\AddOns\StorylineClassic\Artwork\QuestFrame\UI-QuestTitleHighlight" alphaMode="ADD"/>
