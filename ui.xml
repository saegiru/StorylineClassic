	<Button name="Storyline_MultiChoiceButton" inherits="Storyline_InvisibleButton" virtual="true">
		<Size x="0" y="20"/>
		<Layers>
			<Layer level="OVERLAY">
				<FontString name="Text" inherits="GameFontNormal" justifyH="LEFT" justifyV="MIDDLE" setAllPoints="true">
					<Anchors>
						<Anchor point="LEFT">
							<Offset>
								<AbsDimension x="0" y="0"/>
							</Offset>
						</Anchor>
						<Anchor point="RIGHT">
							<Offset>
								<AbsDimension x="0" y="0"/>
							</Offset>
						</Anchor>
					</Anchors>
					<Color r="0.95" g="0.75" b="0"/>
				</FontString>
			</Layer>
		</Layers>
		<Scripts>
			<OnEnter>
				self.Text:SetTextColor(0.95, 0.95, 0.95);
			</OnEnter>
			<OnLeave>
				self.Text:SetTextColor(0.95, 0.75, 0);
			</OnLeave>
			<OnMouseDown>
				self.Text:SetPoint("LEFT", 2, -2);
				self.Text:SetPoint("RIGHT", 2, -2);
			</OnMouseDown>
			<OnMouseUp>
				self.Text:SetPoint("LEFT", 0, 0);
				self.Text:SetPoint("RIGHT", 0, 0);
			</OnMouseUp>
		</Scripts>
	</Button>

