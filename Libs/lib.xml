<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.blizzard.com/wow/ui/">
<!--
	<Script file="LibStub\LibStub.lua"/>
	<Include file="AceAddon-3.0\AceAddon-3.0.xml"/>
	<Include file="AceConsole-3.0\AceConsole-3.0.xml"/>-->

	<GameTooltip name="Storyline_MainTooltip" frameStrata="TOOLTIP" hidden="false" parent="UIParent" inherits="GameTooltipTemplate"/>

	<!-- Common close button  -->
	<Button name="Storyline_CloseButton" inherits="UIPanelCloseButton" virtual="true">
		<Scripts><!--
			<OnEnter>
				Storyline_RefreshTooltipForFrame(self);
			</OnEnter>
			<OnLeave>
				Storyline_MainTooltip:Hide();
			</OnLeave>-->
			<PostClick>
				PlaySound("gsCharacterSelection");
			</PostClick>
		</Scripts>
	</Button>

	<!-- Common red Wow button template  -->
	<Button name="Storyline_CommonButton" inherits="UIPanelButtonTemplate" virtual="true">
		<Size x="90" y="20"/>
		<Scripts><!--
			<OnEnter>
				Storyline_RefreshTooltipForFrame(self);
			</OnEnter>
			<OnLeave>
				Storyline_MainTooltip:Hide();
			</OnLeave>-->
			<PostClick>
				PlaySound("gsCharacterSelection");
			</PostClick>
		</Scripts>
	</Button>

	<!-- Checkbox with small label and TT compatible -->
	<CheckButton name="Storyline_CheckBox" inherits="OptionsCheckButtonTemplate" virtual="true" checked="false">
		<Scripts>
			<OnLoad>
				getglobal(self:GetName().."Text"):SetFontObject(GameFontNormalSmall);
				this.Text = getglobal(self:GetName().."Text");
			</OnLoad><!--
			<OnEnter>
				Storyline_RefreshTooltipForFrame(self);
			</OnEnter>
			<OnLeave>
				Storyline_MainTooltip:Hide();
			</OnLeave>-->
			<PostClick>
				PlaySound("gsCharacterSelection");
			</PostClick>
		</Scripts>
	</CheckButton>

	<!-- Common icon button  -->
	<Button name="Storyline_IconButton" virtual="true">
		<Size x="26" y="26"/>
		<Layers>
			<Layer level="OVERLAY">
				<Texture name="$parentIcon" file="Interface\AddOns\StorylineClassic\Artwork\Icons\INV_Misc_QuestionMark">
					<Size x="26" y="26"/>
					<Anchors>
						<Anchor point="CENTER">
							<Offset>
								<AbsDimension x="0" y="0"/>
							</Offset>
						</Anchor>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
		<NormalTexture file="Interface\AddOns\StorylineClassic\Artwork\AchievementFrame\UI-Achievement-IconFrame">
			<Size x="65" y="65"/>
			<Anchors>
				<Anchor point="CENTER">
					<Offset>
						<AbsDimension x="14" y="-14"/>
					</Offset>
				</Anchor>
			</Anchors>
		</NormalTexture>
		<PushedTexture file="Interface\AddOns\StorylineClassic\Artwork\Buttons\UI-EmptySlot">
			<Size x="50" y="50"/>
			<Anchors>
				<Anchor point="CENTER">
					<Offset>
						<AbsDimension x="0" y="0"/>
					</Offset>
				</Anchor>
			</Anchors>
		</PushedTexture>
		<DisabledTexture file="Interface\AddOns\StorylineClassic\Artwork\Buttons\UI-EmptySlot-Disabled">
			<Size x="47" y="47"/>
			<Anchors>
				<Anchor point="CENTER">
					<Offset>
						<AbsDimension x="0" y="0"/>
					</Offset>
				</Anchor>
			</Anchors>
		</DisabledTexture>
		<HighlightTexture alphaMode="ADD" file="Interface\AddOns\StorylineClassic\Artwork\Buttons\UI-Quickslot-Depress"/>
		<Scripts><!--
			<OnEnter>
				Storyline_RefreshTooltipForFrame(self);
			</OnEnter>
			<OnLeave>
				Storyline_MainTooltip:Hide();
			</OnLeave>-->
			<PostClick>
				PlaySound("gsCharacterSelection");
			</PostClick>
		</Scripts>
	</Button>

	<Slider name="Storyline_Slider" inherits="OptionsSliderTemplate" virtual="true">
		<Size x="140" y="15"/>
		<Layers>
			<Layer level="OVERLAY">
				<FontString name="$parentValText" inherits="GameFontNormal" justifyH="CENTER" text="[value]">
					<Size x="0" y="10"/>
					<Anchors>
						<Anchor point="LEFT"/>
						<Anchor point="RIGHT"/>
					</Anchors>
					<Color r="0.95" g="0.95" b="0.95"/>
				</FontString>
			</Layer>
		</Layers>
		<Scripts><!--
			<OnEnter>
				Storyline_RefreshTooltipForFrame(self);
			</OnEnter>
			<OnLeave>
				Storyline_MainTooltip:Hide();
			</OnLeave>-->
		</Scripts>
	</Slider>

	<!-- Invisible button  -->
	<Button name="Storyline_InvisibleButton" virtual="true">
		<Size x="90" y="20"/>
		<Scripts><!--
			<OnEnter>
				Storyline_RefreshTooltipForFrame(self);
			</OnEnter>
			<OnLeave>
				Storyline_MainTooltip:Hide();
			</OnLeave>-->
		</Scripts>
	</Button>

	<!-- Titled edit box, TRP2 style -->
	<EditBox name="Storyline_TitledEditBox" letters="0" historyLines="10" autoFocus="false" virtual="true">
		<Size x="100" y="18"/>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture name="$parentLeft" file="Interface\AddOns\StorylineClassic\Artwork\Common\Common-Input-Border">
					<Size x="12" y="28"/>
					<Anchors>
						<Anchor point="TOPLEFT">
							<Offset>
								<AbsDimension x="-5" y="0"/>
							</Offset>
						</Anchor>
					</Anchors>
					<TexCoords left="0" right="0.09375" top="0" bottom="1.0"/>
				</Texture>
				<Texture name="$parentRight" file="Interface\AddOns\StorylineClassic\Artwork\Common\Common-Input-Border">
					<Size x="12" y="28"/>
					<Anchors>
						<Anchor point="TOPRIGHT">
							<Offset>
								<AbsDimension x="5" y="0"/>
							</Offset>
						</Anchor>
					</Anchors>
					<TexCoords left="0.90625" right="1.0" top="0" bottom="1.0"/>
				</Texture>
				<Texture name="$parentCenter" file="Interface\AddOns\StorylineClassic\Artwork\Common\Common-Input-Border">
					<Size x="0" y="28"/>
					<Anchors>
						<Anchor point="LEFT" relativeTo="$parentLeft" relativePoint="RIGHT"/>
						<Anchor point="RIGHT" relativeTo="$parentRight" relativePoint="LEFT"/>
					</Anchors>
					<TexCoords left="0.09375" right="0.90625" top="0" bottom="1.0"/>
				</Texture>
			</Layer>
			<Layer level="OVERLAY">
				<FontString name="$parentText" text="[title]" inherits="GameFontNormalSmall" justifyH="LEFT">
					<Anchors>
						<Anchor point="TOPLEFT">
							<Offset>
								<AbsDimension x="5" y="5"/>
							</Offset>
						</Anchor>
						<Anchor point="TOPRIGHT">
							<Offset>
								<AbsDimension x="0" y="5"/>
							</Offset>
						</Anchor>
					</Anchors>
					<Color r="0.95" g="0.75" b="0.1"/>
				</FontString>
			</Layer>
		</Layers>
		<FontString inherits="GameFontWhite"/>
		<Scripts><!--
			<OnEscapePressed>
				self:ClearFocus()
			</OnEscapePressed>
			<OnEditFocusGained>
				if self.disabled then
				self:ClearFocus();
				else
				self:HighlightText();
				end
			</OnEditFocusGained>
			<OnEditFocusLost>
				self:HighlightText(0,0);
			</OnEditFocusLost>
			<OnEnter>
				Storyline_RefreshTooltipForFrame(self);
			</OnEnter>
			<OnLeave>
				Storyline_MainTooltip:Hide();
			</OnLeave>-->
		</Scripts>
	</EditBox>

	<!-- Tutorial style hovered frame -->
	<Frame name="Storyline_HoveredFrame" inherits="GlowBoxTemplate" virtual="true">
		<Size x="220" y="100"/>
		<Layers>
			<Layer level="ARTWORK">
				<Texture parentKey="ArrowUP" inherits="HelpPlateArrowDOWN" hidden="true">
					<Size x="53" y="21"/>
					<Anchors>
						<Anchor point="TOP" relativePoint="BOTTOM">
							<Offset>
								<AbsDimension x="0" y="3"/>
							</Offset>
						</Anchor>
					</Anchors>
				</Texture>
				<Texture parentKey="ArrowDOWN" inherits="HelpPlateArrowUP" hidden="true">
					<Size x="53" y="21"/>
					<Anchors>
						<Anchor point="BOTTOM" relativePoint="TOP">
							<Offset>
								<AbsDimension x="0" y="-3"/>
							</Offset>
						</Anchor>
					</Anchors>
				</Texture>
				<Texture parentKey="ArrowRIGHT" inherits="HelpPlateArrowDOWN" hidden="true">
					<Size x="53" y="21"/>
					<Anchors>
						<Anchor point="RIGHT" relativePoint="LEFT">
							<Offset>
								<AbsDimension x="3" y="0"/>
							</Offset>
						</Anchor>
					</Anchors>
				</Texture>
				<Texture parentKey="ArrowLEFT" inherits="HelpPlateArrowDOWN" hidden="true">
					<Size x="53" y="21"/>
					<Anchors>
						<Anchor point="LEFT" relativePoint="RIGHT">
							<Offset>
								<AbsDimension x="-3" y="0"/>
							</Offset>
						</Anchor>
					</Anchors>
				</Texture>
			</Layer>
			<Layer level="BORDER">
				<Texture parentKey="ArrowGlowUP" inherits="HelpPlateArrow-GlowDOWN" hidden="true" alphaMode="ADD" alpha="0.5">
					<Size x="53" y="21"/>
					<Anchors>
						<Anchor point="TOP" relativePoint="BOTTOM">
							<Offset>
								<AbsDimension x="0" y="3"/>
							</Offset>
						</Anchor>
					</Anchors>
				</Texture>
				<Texture parentKey="ArrowGlowDOWN" inherits="HelpPlateArrow-GlowUP" hidden="true" alphaMode="ADD" alpha="0.5">
					<Size x="53" y="21"/>
					<Anchors>
						<Anchor point="BOTTOM" relativePoint="TOP">
							<Offset>
								<AbsDimension x="0" y="-3"/>
							</Offset>
						</Anchor>
					</Anchors>
				</Texture>
				<Texture parentKey="ArrowGlowRIGHT" inherits="HelpPlateArrow-GlowDOWN" hidden="true" alphaMode="ADD" alpha="0.5">
					<Size x="53" y="21"/>
					<Anchors>
						<Anchor point="RIGHT" relativePoint="LEFT">
							<Offset>
								<AbsDimension x="3" y="0"/>
							</Offset>
						</Anchor>
					</Anchors>
				</Texture>
				<Texture parentKey="ArrowGlowLEFT" inherits="HelpPlateArrow-GlowDOWN" hidden="true" alphaMode="ADD" alpha="0.5">
					<Size x="53" y="21"/>
					<Anchors>
						<Anchor point="LEFT" relativePoint="RIGHT">
							<Offset>
								<AbsDimension x="-3" y="0"/>
							</Offset>
						</Anchor>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
		<Scripts>
			<OnLoad><!--
				SetClampedTextureRotation(self.ArrowLEFT, 270);
				SetClampedTextureRotation(self.ArrowRIGHT, 90);
				SetClampedTextureRotation(self.ArrowGlowLEFT, 270);
				SetClampedTextureRotation(self.ArrowGlowRIGHT, 90);-->
			</OnLoad>
		</Scripts>
	</Frame>

	<!-- Resize button -->
	<Button name="Storyline_ResizeButton" virtual="true">
		<Size x="32" y="32"/>
		<Scripts>
			<OnLoad><!--
				Storyline_API.lib.initResize(self);-->
			</OnLoad>
		</Scripts>
		<NormalTexture file="Interface\AddOns\StorylineClassic\Artwork\Buttons\UI-Panel-SmallerButton-Up">
			<TexCoords left="1" right="0" top="0" bottom="1"/>
		</NormalTexture>
		<PushedTexture file="Interface\AddOns\StorylineClassic\Artwork\Buttons\UI-Panel-SmallerButton-Down">
			<TexCoords left="1" right="0" top="0" bottom="1"/>
		</PushedTexture>
		<HighlightTexture file="Interface\AddOns\StorylineClassic\Artwork\Buttons\UI-Panel-MinimizeButton-Highlight" alphaMode="ADD"/>
	</Button>

	<Frame name="Storyline_ResizeShadowFrame" parent="UIParent" frameStrata="FULLSCREEN" hidden="true" resizable="true">
		<Backdrop edgeFile="Interface\AddOns\StorylineClassic\Artwork\Glues\Common\TextPanel-Border" tile="true" bgFile="Interface\AddOns\StorylineClassic\Artwork\DialogFrame\UI-DialogBox-Background">
			<EdgeSize><AbsValue val="32"/></EdgeSize><TileSize><AbsValue val="400"/></TileSize>
			<BackgroundInsets><AbsInset left="5" right="5" top="5" bottom="5"/></BackgroundInsets>
		</Backdrop>
		<Layers>
			<Layer level="OVERLAY">
				<FontString name="Storyline_ResizeShadowFrameText" inherits="GameFontNormal" justifyH="CENTER" justifyV="MIDDLE" setAllPoints="true">
					<Color b="1.0" r="1.0" g="1.0"/>
				</FontString>
			</Layer>
		</Layers>
	</Frame>
<!--
	<Script file="lib.lua"/>
-->
	<Frame parent="UIParent" name="Storyline_EventFrame">
		<Anchors>
			<Anchor point="CENTER"/>
		</Anchors>
		<Scripts>
			<OnEvent function="Storyline_EventDispatcher"/>
		</Scripts>
	</Frame>

</Ui>